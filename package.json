{
	"name": "trpc-snapshot",
	"version": "0.0.0",
	"type": "module",
	"description": "snapshot your tRPC procedure's output types",
	"main": "dist/index.cjs",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": "./dist/index.js",
			"default": "./dist/index.cjs"
		}
	},
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"example": "nodemon --exec \"tsx src/index.ts\" -e ts --ignore snapshots",
		"build": "tsup",
		"format": "dprint fmt",
		"lint": "run-p lint:*",
		"lint:format": "dprint check",
		"lint:types": "tsc",
		"lint:js": "eslint .",
		"prepublishOnly": "pnpm build",
		"test": "vitest"
	},
	"dependencies": {
		"@sinclair/typebox": "^0.25.8",
		"@trpc/server": "^10.0.0-rc.7",
		"@ts-morph/bootstrap": "^0.17.0",
		"ts-morph": "^16.0.0"
	},
	"devDependencies": {
		"@babel/core": "7.20.2",
		"@prisma/client": "4.6.1",
		"@sachinraja/eslint-config": "0.2.0",
		"@trpc/client": "10.0.0-rc.7",
		"@types/babel__core": "7.1.20",
		"@types/node": "18.11.9",
		"babel-plugin-spectypes": "^2.1.7",
		"dprint": "0.32.2",
		"eslint": "8.27.0",
		"nodemon": "2.0.20",
		"npm-run-all": "4.1.5",
		"prisma": "4.6.1",
		"tsup": "6.4.0",
		"tsx": "3.12.1",
		"typescript": "4.8.4",
		"undici": "5.12.0",
		"vite": "3.2.3",
		"vitest": "0.25.1",
		"zod": "3.19.1"
	},
	"sideEffects": false,
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"format": [
			"esm",
			"cjs"
		],
		"dts": {
			"resolve": true
		},
		"splitting": true,
		"clean": true
	},
	"packageManager": "pnpm@7.2.0"
}
